set defsearch=google
set maxitems=10
set ignorecase
set showstatuslinks=2
set maxitems=10

set noerrorbells
set focuscontent

cnoremap <Up> <S-Tab>
cnoremap <Down> <Tab>

noremap j 5j
noremap k 5k

noremap <S-j> 100%
noremap <S-k> gg

noremap h <C-p>
noremap l <C-n>

